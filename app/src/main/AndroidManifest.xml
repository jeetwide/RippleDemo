<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeetendraachtani.rippledemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Ripple.ActRippleMaterial" />
        <activity
            android:name=".Ripple.DemoActivity"
            android:label="@string/app_name">

        </activity>
        <activity
            android:name=".Ripple.DemoListActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".Ripple.DemoRecyclerActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".ActWelcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".BottomBar.ActWelcomeBottomBar"
            android:launchMode="singleInstance">
        </activity>
        <activity android:name=".BottomBar.ThreeTabsActivity" />
        <activity android:name=".BottomBar.IconsOnlyActivity" />
